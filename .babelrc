{
  "presets": [
    "@babel/preset-env",
    [
      "@babel/preset-react",
      {
        "runtime": "automatic"
      }
    ]
  ],
  "plugins": [
    ["@babel/plugin-transform-runtime"],
    [
      "babel-plugin-styled-components",
      {
        "ssr": true,
        "displayName": false,
        "fileName": false,
        "minify": true,
        "pure": true,
        "transpileTemplateLiterals": true
      }
    ]
  ]
}
